@model Domain.ViewModels.Account.SendResetPasswordRequistViewModel
@{
    ViewBag.TitlePage = "Send resent mail";
}
@using (Html.BeginForm("SendResetPasswordRequist", "Account", FormMethod.Post))
{
    
    <div asp-validation-summary="All" class="text-danger"></div>
    <div class="form-field">
        <label asp-for="Email" class="form-label"></label>
        <input asp-for="Email" class="form-control" aria-describedby="emailHelp">
    </div>
    <div class="form-field">
        <button type="submit" class="btn btn-primary">Submit</button>
    </div>
}